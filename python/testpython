import csv
data=[]
i=0
with open('C:/Users/Alison/Documents/Research/S3/realtime-arrivals.excerpt.csv', 'rb') as csvfile:
	spamreader = csv.reader(csvfile, delimiter=',')
	for row in spamreader:
		data.append([])
		for ele in row:
			data[i].append(ele)
		i=i+1

